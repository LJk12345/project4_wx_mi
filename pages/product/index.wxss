} */

.receiveName {
  display: flex;
  margin-bottom: 5px;
}

.receiveName span {
  height: 80%;
  font-size: 10px;
  border: 1px solid #b4731f;
  color: #b4731f;
  padding: 1px 4px;
  margin: -4px 0 0 6px;
